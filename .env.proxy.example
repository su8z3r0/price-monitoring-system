# Proxy Configuration
# Set to false to disable proxy usage entirely (faster, direct requests)
PROXY_ENABLED=true

# Individual Proxy Providers (can enable/disable each)
PROXY_PROVIDER_GEONODE_ENABLED=true
PROXY_PROVIDER_PROXIFLY_ENABLED=true
PROXY_PROVIDER_FREEPROXY24_ENABLED=false
PROXY_PROVIDER_MANUAL_ENABLED=false

# Manual Proxy List (comma-separated, only used if PROXY_PROVIDER_MANUAL_ENABLED=true)
# PROXY_LIST="premium-proxy1.example.com:8080,premium-proxy2.example.com:8080"

# Proxy Settings
PROXY_TIMEOUT=30
PROXY_MAX_RETRIES=8
PROXY_VALIDATION_LIMIT=100
PROXY_CACHE_TTL=3600

# User Agent
PROXY_USER_AGENT="Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36"
